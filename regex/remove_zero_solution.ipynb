   "source": [
    "# Write a Python program to remove leading zeros from an IP address.\n",
    "# Input\n",
    "# \"216.08.094.196\"\n",
    "# Output\n",
    "# 216.8.94.196\n",
    "\n",
    "import re\n",
    "ip = \"216.08.094.196\"\n",
    "string = re.sub('\\.[0]*', '.', ip)\n",
    "print(string)"
   ]
